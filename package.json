{
  "name": "sails-memory",
  "version": "0.11.0",
  "description": "Non-persistent in-memory adapter for Sails.js / Waterline",
  "main": "lib/adapter.js",
  "scripts": {
    "test": "node test/runner.js",
    "prepublish": "npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-memory.git"
  },
  "keywords": [
    "waterline",
    "database",
    "memory",
    "sails",
    "sails.js",
    "sailsjs"
  ],
  "author": "Mike McNeil",
  "contributors": [
    {
      "name": "Cody Stoltman"
    }
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "async": "^3.2.4",
    "lodash": "^4.17.21",
    "waterline-criteria": "https://github.com/carlosgalveias/waterline-criteria.git#v1.0.2",
    "waterline-cursor": "0.0.6",
    "waterline-errors": "0.10.1"
  },
  "devDependencies": {
    "captains-log": "^2.0.3",
    "mocha": "^10.2.0"
  },
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.0",
    "interfaces": [
      "semantic",
      "queryable",
      "associations"
    ],
    "features": [
      "cross-adapter",
      "unique",
      "autoIncrement.sequential"
    ]
  }
}
